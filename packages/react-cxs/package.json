{
  "name": "react-cxs",
  "version": "3.0.3",
  "description": "Alternative React.createElement function which allows style objects to be passed to the className prop to generate CSS using cxs",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "test": "ava test -v"
  },
  "keywords": [
    "react",
    "jsx",
    "css",
    "css-in-js"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cxs": "latest"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
